<Project>
	<PropertyGroup>
		<LangVersion>8.0</LangVersion>
		<NullableContextOptions>enable</NullableContextOptions>
		<WarningsAsErrors>CS8612;CS8620;CS8600;CS8615;CS8619;CS8602;CS8603;CS8625;CS8618;CS8601;CS8604;CS8629;CS8614;CS8613;CS8609</WarningsAsErrors>
		
		<Title>Fuxion</Title>
		<Authors>Oscar Jimenez</Authors>

		<Product>Fuxion</Product>
		<ProductName>Fuxion</ProductName>

		<PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
		<PackageLicenseUrl>https://github.com/osjimenez/Fuxion/raw/master/LICENSE</PackageLicenseUrl>
		<PackageProjectUrl>https://github.com/osjimenez/Fuxion</PackageProjectUrl>
		<PackageIconUrl>https://github.com/osjimenez/Fuxion/raw/master/res/logo/fuxion.png</PackageIconUrl>

		<IncludeSymbols>true</IncludeSymbols>

		<PackageOutputPath>$(SolutionDir)..\..\PackOutput</PackageOutputPath>
	</PropertyGroup>
</Project>